{
  "name": "aep-custom-scripts",
  "version": "2.0.0",
  "description": "TypeScript-based data fetchers for Adobe Experience Platform (AEP) Data Collection",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && node scripts/buildWithEsbuild.js",
    "clean": "rm -rf dist build .temp",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "adobe",
    "aep",
    "data-collection",
    "analytics",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "esbuild": "^0.27.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5.7.2"
  }
}
